ui_print("");
ui_print("+----------------------------+");
ui_print("|   Installing YacineKernel  |");
ui_print("|              V1            |");
ui_print("+----------------------------+");
ui_print("");

package_extract_file("files/boot.img", "/dev/block/by-name/boot");
package_extract_file("files/dtbo.img", "/dev/block/by-name/dtbo");

ui_print("");
ui_print("+----------------------------+");
ui_print("|   Installation Success     |");
ui_print("+----------------------------+");
ui_print("");
ui_print("You may now reboot your device.");
ui_print("");
